{
  "name": "dotenv",
  "version": "5.0.0",
  "description": "Loads environment variables from .env file",
  "main": "build/main.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel lib/main.js -o build/main.js",
    "pretest": "npm run build && npm run lint",
    "test": "lab test/* -r lcov | coveralls",
    "lint": "standard",
    "postlint": "npm run lint-md",
    "lint-md": "standard-markdown",
    "precommit": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/motdotla/dotenv.git"
  },
  "keywords": [
    "dotenv",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "readmeFilename": "README.md",
  "author": "scottmotte",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "babel": "5.8.23",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^2.11.9",
    "husky": "^0.14.3",
    "lab": "11.1.0",
    "should": "11.1.1",
    "sinon": "1.17.6",
    "standard": "8.4.0",
    "standard-markdown": "2.2.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=0.12"
  },
  "standard": {
    "ignore": [
      "/build"
    ]
  }
}
